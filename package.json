{
  "name": "Kindle_review_app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "cross-env NODE_ENV=production concurrently \"npm run client-prod\" \"npm run server-prod\"",
    "client-prod": "webpack --mode production",
    "server-prod": "npm run build && node ./build/index.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run client-dev\" \"npm run server-dev\"",
    "client-dev": "webpack-dev-server --open --mode development --watch",
    "server-dev": "cross-env NODE_ENV=development nodemon server/index.js --exec babel-node",
    "test": "cross-env NODE_ENV=test concurrently \"npm run client-test\" \"npm run server-test\"",
    "client-test": "jest --coverage",
    "server-test": "cross-env NODE_ENV=test nyc _mocha server/test --exit",
    "build": "npm run clean && npm run server-build",
    "server-build": "babel -d ./build ./server -s",
    "clean": "rm -rf build && mkdir build",
    "open-cov": "open coverage/lcov-report/index.html"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "rootDir": "client",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "cors": "^2.8.4",
    "cross-env": "^5.2.1",
    "dotenv": "^6.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.15.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mdbreact": "^4.23.0",
    "mongoose": "^5.2.2",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-calendar": "^2.19.2",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
