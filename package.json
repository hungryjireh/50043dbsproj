    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^3.2.0",
    "chai-http": "^1.0.0",
    "codecov": "^3.0.4",
    "css-loader": "^1.0.1",
    "enzyme": "^3.10.0",
